-- GUN MODS
local Tab = Window:CreateTab("Gun Mods", 4483362458) -- Title, Image
local Section = Tab:CreateSection("Gun Mods (MAY SAY CALLBACK ERROR BUT THEY WORK!)")
local Button = Tab:CreateButton({
	Name = "Full Auto",
	Callback = function()
		for i, v in pairs(replicationstorage.Weapons:GetDescendants()) do
			if v.Name == "Auto" then
				v.Value = true
			end
		end
	end,
})
local Button = Tab:CreateButton({
	Name = "No Recoil",
	Callback = function()
		for i, v in pairs(replicationstorage.Weapons:GetDescendants()) do
			if v.Name == "RecoilControl" then
				v.Value = 0
			end
		end
	end,
})
local Button = Tab:CreateButton({
	Name = "No Bullet Spread",
	Callback = function()
		for i, v in pairs(replicationstorage.Weapons:GetDescendants()) do
			if v.Name == "MaxSpread" then
				v.Value = 0
			end
		end
	end,
})
local Button = Tab:CreateButton({
	Name = "No Reload",
	Callback = function()
		for i, v in pairs(replicationstorage.Weapons:GetDescendants()) do
			if v.Name == "ReloadTime" then
				v.Value = 1
			 end
		end
	end,
})
local Button = Tab:CreateButton({
	Name = "Fire Rate",
	Callback = function()
		for i, v in pairs(replicationstorage.Weapons:GetDescendants()) do
			if v.Name == "FireRate" then
				v.Value = 0.05
			end
		end
	end,
})

-- AUTO FARM
local Tab = Window:CreateTab("AutoFarm", 4483362458) -- Title, Image
local Section = Tab:CreateSection("AutoFarm Script (Paste into brower then put script into autoexec)")
local Button = Tab:CreateButton({
	Name = "AutoFarm",
	Callback = function()
		setclipboard("https://raw.githubusercontent.com/Dark-Scripts/Arsenal-Scripts/main/AutoFarm")
		Rayfield:Notify({
			Title = "Link Copied",
			Content = "Link Copied (Paste into brower then put script into autoexec)",
			Duration = 6.5,
			Image = 4483362458,
			Actions = { -- Notification Buttons
				Ignore = {
					Name = "ok",
					Callback = function()
						print("FUCK")
					end
				},
			},
		})
	end,
})

