and d.Character:FindFirstChild("Humanoid").Health>0 then if h.Aimbot_TeamCheck==true and d.Team~=e.Team or h.Aimbot_TeamCheck==false then local d=d.Character;local a,e=a:WorldToViewportPoint(d[h.Aimbot_AimPart].Position)if e then local a=(Vector2.new(f.X,f.Y)-Vector2.new(a.X,a.Y)).Magnitude;if a<b and a<h.Aimbot_FOV_Radius then b=a;c=d end end end end end;if c~=nil and c:FindFirstChild("HumanoidRootPart")and c:FindFirstChild("Humanoid")and c:FindFirstChild("Humanoid").Health>0 then a.CFrame=CFrame.new(a.CFrame.Position,c[h.Aimbot_AimPart].Position)end end end)end})local a=k:CreateButton({Name="Silent Aim",Callback=function()loadstring(game:HttpGet('https://pastebin.com/raw/EnSX9RiH'))()end})local a=k:CreateDropdown({Name="Aim Part",Options={"Head","UpperTorso","LowerTorso","Random"},CurrentOption="Head",Flag="aimparts",Callback=function(a)if a=="Random"then local a=g[math.random(#g)]h.Aimbot_AimPart=a else h.Aimbot_AimPart=a end end})local a=k:CreateToggle({Name="Teamcheck",CurrentValue=true,Flag="Toggle1",Callback=function(a)if a==true then h.Aimbot_TeamCheck=true elseif a==false then h.Aimbot_TeamCheck=false end end})local a=k:CreateSection("FOV Settings")local a=k:CreateSlider({Name="FOV Circle Radius",Range={1,1000},Increment=1,Suffix="",CurrentValue=200,Flag="Slider1",Callback=function(a)h.Aimbot_FOV_Radius=a;i.Radius=h.Aimbot_FOV_Radius end})local a=k:CreateSlider({Name="FOV Circle Thickness",Range={1,100},Increment=1,Suffix="",CurrentValue=1,Flag="Slider2",Callback=function(a)i.Thickness=a end})local a=k:CreateSlider({Name="FOV Circle Transparency",Range={0,1},Increment=0.1,Suffix="",CurrentValue=1,Flag="Trans",Callback=function(a)i.Transparency=a end})local a=k:CreateToggle({Name="FOV Circle Filled",CurrentValue=false,Flag="filled",Callback=function(a)if a==true then i.Filled=true elseif a==false then i.Filled=false end end})local a=k:CreateColorPicker({Name="Color Picker",Color=Color3.fromRGB(255,255,255),Flag="ColorPicker1",Callback=function(a)h.Aimbot_FOV